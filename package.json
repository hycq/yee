    "babel-polyfill": "^6.26.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.13",
    "compression": "^1.6.2",
    "cssnano": "^3.7.4",
    "d3": "3.5.17",
    "d3-tooltip": "^0.0.1",
    "debug": "^2.2.0",
    "echarts": "^4.0.4",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.7.0",
    "jquery": "^3.3.1",
